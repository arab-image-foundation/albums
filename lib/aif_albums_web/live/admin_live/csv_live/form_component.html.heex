<div>
  <h2><%= @title %></h2>

  <.form
    let={f}
    for={@changeset}
    id="csv-form"
    phx-target={@myself}
    phx-change="validate"
    phx-submit="save">
  
    <div>
      <%= live_file_input @uploads.csv_file %>
      <%= submit "Save", phx_disable_with: "Saving..." %>
    </div>
  </.form>
</div>
